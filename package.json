{
  "name": "zoom.ts",
  "version": "7.5.0",
  "description": "A lightweight TypeScript library for image zooming, as seen on Medium.",
  "keywords": [
    "zoom.ts",
    "zoom.js",
    "image",
    "zoom",
    "zooming",
    "medium"
  ],
  "homepage": "https://www.michael-bull.com/projects/zoom.ts",
  "bugs": {
    "url": "https://github.com/michaelbull/zoom.ts/issues"
  },
  "license": "ISC",
  "author": {
    "name": "Michael Bull",
    "url": "https://www.michael-bull.com"
  },
  "main": "dist/zoom.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelbull/zoom.ts"
  },
  "scripts": {
    "clean": "rm -rf dist/* && rm -rf reports/*",
    "start": "npm run clean && cross-env NODE_ENV=development rollup --config --watch",
    "build": "npm run clean && rollup --config",
    "test": "karma start test/karma.conf.js --singleRun",
    "test:watch": "karma start test/karma.conf.js",
    "coveralls": "cat ./reports/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dist": "npm run clean && cross-env NODE_ENV=production rollup --config",
    "lint": "tslint lib/**/*.ts test/**/*.ts"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.47",
    "coveralls": "2.13.1",
    "cross-env": "4.0.0",
    "cssnano": "3.10.0",
    "jasmine-core": "2.6.1",
    "karma": "1.7.0",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-spec-reporter": "0.0.31",
    "karma-typescript": "3.0.1",
    "node-sass": "4.5.2",
    "rollup": "0.41.6",
    "rollup-plugin-closure-compiler-js": "1.0.4",
    "rollup-plugin-livereload": "0.4.0",
    "rollup-plugin-sass": "0.5.1",
    "rollup-plugin-serve": "0.3.0",
    "rollup-plugin-typescript2": "0.4.1",
    "rollup-plugin-uglify": "1.0.2",
    "rollup-watch": "3.2.2",
    "tslib": "1.7.0",
    "tslint": "5.2.0",
    "typescript": "2.3.2"
  }
}
